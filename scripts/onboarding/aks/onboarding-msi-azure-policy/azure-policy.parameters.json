{
    "resourceTagValues": {
      "type": "object"
    },
    "workspaceResourceId": {
      "type": "string"
    },
    "workspaceRegion": {
      "type": "string"
    },
    "enableSyslog": {
      "type": "boolean",
      "defaultValue": false
    },
    "syslogLevels": {
      "type": "array",
      "defaultValue": [
        "Debug",
        "Info",
        "Notice",
        "Warning",
        "Error",
        "Critical",
        "Alert",
        "Emergency"
      ]
    },
    "syslogFacilities": {
      "type": "array",
      "defaultValue": [
          "auth",
          "authpriv",
          "cron",
          "daemon",
          "mark",
          "kern",
          "local0",
          "local1",
          "local2",
          "local3",
          "local4",
          "local5",
          "local6",
          "local7",
          "lpr",
          "mail",
          "news",
          "syslog",
          "user",
          "uucp"
      ]
    },
    "dataCollectionInterval": {
      "type": "string",
      "defaultValue": "1m"
    },
    "namespaceFilteringModeForDataCollection": {
      "type": "string",
      "defaultValue": "Off",
      "allowedValues": [
        "Off",
        "Include",
        "Exclude"
      ]
    },
    "namespacesForDataCollection": {
      "type": "array",
      "defaultValue": [
        "kube-system", "gatekeeper-system", "azure-arc"
      ]
    }
}
